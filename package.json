{
	"name": "curse-my-discord",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "tsx watch --env-file .env --watch src/index.ts",
		"prestart": "pnpm build",
		"start": "node --env-file .env dist/index.js",
		"build": "tsup",
		"check": "tsc --noEmit && biome check && knip",
		"fix": "biome check --fix"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@types/node": "^25.0.3",
		"arktype": "^2.1.29",
		"discord.js": "^14.25.1",
		"knip": "^5.78.0",
		"tsup": "^8.5.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"winston": "^3.19.0",
		"winston-transport": "^4.9.0"
	},
	"packageManager": "pnpm@10.27.0"
}
